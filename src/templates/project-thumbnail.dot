<a href="/projects/{{=it.slug}}" id="project-thumb-{{=it.slug}}" class="project-item {{=it.size}}">
  <div class="project-visual">
    {{? it.thumbnail }}
      <img src="{{=it.thumbnail}}" alt="">
    {{?}}
  </div>
  <div class="project-desc">
    <h2>{{=it.name}}</h2>
    <p>{{=it.desc}}</p>
  </div>
</a>
