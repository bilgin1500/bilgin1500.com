@font-face
  font-family 'MuseoSans-300'
  src url($fonts-folder-museosans + '2AD11A_1_0.eot')
  src url($fonts-folder-museosans + '2AD11A_1_0.eot?#iefix') format('embedded-opentype'),
  url($fonts-folder-museosans + '2AD11A_1_0.woff') format('woff'),
  url($fonts-folder-museosans + '2AD11A_1_0.ttf') format('truetype')

