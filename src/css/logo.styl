@import "assets/fonts"
json('../database/settings.json')

#logo
  position fixed
  top 40px
  left 40px
  width 300px
  height 40px
  visibility hidden
  overflow hidden
  z-index 111
  user-select none

  .text
    position absolute
    display block
    font-family AvenirHeavy
    font-size fHuge
    color #000
    
    .char
      visibility hidden

  .cursor
    color spotColor
    width 25px
