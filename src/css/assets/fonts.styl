  url(fontsFolderMuseo + '350BF7_0_0.woff2') format('woff2'),
  url(fontsFolderMuseo + '350BF7_0_0.woff') format('woff'),
  url(fontsFolderMuseo + '350BF7_0_0.ttf') format('truetype')
  
museoSans = 'MuseoSans-300', Helvetica, Arial
museoSansB = 'MuseoSans-900', Helvetica, Arial
