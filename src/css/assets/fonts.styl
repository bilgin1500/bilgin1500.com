  url(fontsFolderMuseoSans + '2AD11A_9_0.woff') format('woff'),
  url(fontsFolderMuseoSans + '2AD11A_9_0.ttf') format('truetype')

@font-face
  font-family 'Museo-300'
  src url(fontsFolderMuseo + '350BF7_1_0.eot')
  src url(fontsFolderMuseo + '350BF7_1_0.eot?#iefix') format('embedded-opentype'),
  url(fontsFolderMuseo + '350BF7_1_0.woff2') format('woff2'),
  url(fontsFolderMuseo + '350BF7_1_0.woff') format('woff'),
  url(fontsFolderMuseo + '350BF7_1_0.ttf') format('truetype')

@font-face 
  font-family 'Museo-700'
  src url(fontsFolderMuseo + '350BF7_0_0.eot')
