fontsFolderMuseoSans = '../fonts/museosans/'
fontsFolderMuseo = '../fonts/museo/'

@font-face
  font-family 'MuseoSans-300'
  src url(fontsFolderMuseoSans + '2AD11A_1_0.eot')
  src url(fontsFolderMuseoSans + '2AD11A_1_0.eot?#iefix') format('embedded-opentype'),
  url(fontsFolderMuseoSans + '2AD11A_1_0.woff') format('woff'),
  url(fontsFolderMuseoSans + '2AD11A_1_0.ttf') format('truetype')

@font-face
  font-family 'MuseoSans-300Italic'
  src url(fontsFolderMuseoSans + '2AD11A_6_0.eot')
  src url(fontsFolderMuseoSans + '2AD11A_6_0.eot?#iefix') format('embedded-opentype'),
  url(fontsFolderMuseoSans + '2AD11A_6_0.woff') format('woff'),
  url(fontsFolderMuseoSans + '2AD11A_6_0.ttf') format('truetype')

@font-face
  font-family 'MuseoSans-900'
  src url(fontsFolderMuseoSans + '2AD11A_9_0.eot')
